<?php
/**
 * @file
 * This file attaches the Header Menu JS module
 * to every page.
 */

/**
 * Implements hook_preprocess_page ().
 *
 * Attaches the Header Menu JS module to every
 * page.
 */
function header_menu_preprocess_page (&$variables) {
  $variables ['#attached']['library'][] = 'header_menu/header_menu_library';
}
