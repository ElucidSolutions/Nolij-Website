<?php
/**
 * @file
 * Attaches the Inline SVG library to every page.
 */

/**
 * Implements hook_preprocess_page ().
 *
 * Attaches the Inline SVG library to every page.
 */
function inlinesvg_preprocess_page (&$variables) {
  $variables ['#attached']['library'][] = 'inlinesvg/inlinesvg_library';
}
